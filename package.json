{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "\"# CRUD-API\"",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --silent",
    "test:verbose": "jest",
    "build": "tsc",
    "start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "npm run build && node dist/index.js"
